{
  "name": "readlist-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.13",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "apollo-server-lambda": "^2.21.2",
    "encoding": "^0.1.13",
    "graphql": "^15.5.0",
    "mysql": "^2.18.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^6.15.5",
    "react-scripts": "4.0.3",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/uuid": "^8.3.0",
    "apollo": "^2.32.5",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "class-validator": "^0.13.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "netlify-lambda": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "start": "run-p start:*",
    "start:app": "BROWSER=none react-scripts start",
    "start:func": "NODE_ENV=development netlify-lambda serve ./graphql/functions",
    "build": "run-p build:*",
    "build:app": "react-scripts build",
    "build:lambda": "netlify-lambda build ./graphql/functions",
    "format": "run-s format:*",
    "format:eslint": "eslint --fix  \"**/*.{ts,tsx}\"",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --quiet \"**/*.{ts,tsx}\"",
    "lint:prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "gen-types": "apollo client:codegen ./src/types/generated-types.ts --target=typescript --outputFlat --passthroughCustomScalars",
    "type-check": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
