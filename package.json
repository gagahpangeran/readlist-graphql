{
  "name": "readlist-graphql",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-server-lambda": "^2.21.2",
    "bcryptjs": "^2.4.3",
    "encoding": "^0.1.13",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.31"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-typescript": "^7.13.0",
    "@types/aws-lambda": "^8.10.73",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "netlify-lambda": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "reflect-metadata": "^0.1.13",
    "sql.js": "^1.5.0",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "start": "NODE_ENV=development netlify-lambda serve ./src/functions",
    "build": "netlify-lambda build ./src/functions",
    "format": "run-s format:*",
    "format:eslint": "eslint --fix  \"**/*.{ts,tsx}\"",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --quiet \"**/*.{ts,tsx}\"",
    "lint:prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "migration:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -d ./migrations -n",
    "migration:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
